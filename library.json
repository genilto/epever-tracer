{
    "name": "EpeverTracer",
    "version": "0.0.3",
    "keywords": "epever, tracer, rs485, modbus",
    "description": "Enables communication with Epever Tracer charge controller via Modbus slaves over RS485. Requires an RS485 transceiver.",
    "repository": {
        "type": "git",
        "url": "https://github.com/genilto/epever-tracer.git"
    },
    "homepage": "https://github.com/genilto/epever-tracer",
    "authors": [
        {
            "name": "Genilto Vanzin",
            "url": "https://genilto.com",
            "maintainer": true
        }
    ],
    "license": "GPL-3.0",
    "exclude": "tests",
    "examples": "examples/*/*.cpp",
    "frameworks": "arduino",
    "dependencies": {
        "genilto/SimpleTimer": "^0.0.1",
        "4-20ma/ModbusMaster": "~2.0.1"
    },
    "platforms": [
        "atmelavr",
        "espressif8266",
        "espressif32"
    ]
}
